{
    "manifest_version": 2,
    "name": "模拟HTTP请求Demo",
    "description": "以http://ssfw.xjtu.edu.cn/为例，讲解Chrome插件利用XMLHttpRequest模拟HTTP请求的方法",
    "version": "1.0.0",
	"browser_action": {
		"default_popup": "demo.html"
	},
    "permissions": [
        "https://cas.xjtu.edu.cn/*",
        "http://ssfw.xjtu.edu.cn/*"
    ]
}
